{
  "name": "reactWebpackBoilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for writing React apps. Includes Webpack, Web Dev Server (HMR) and Sass compilation.",
  "main": "",
  "scripts": {
    "prod": "webpack -p --progress --colors --config ./prod.webpack.config.js",
    "dev": "webpack --config ./webpack.config.js && webpack-dev-server --config ./webpack.config.js --inline --hot --content-base ./prod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mattyrichards/react-webpack-boilerplate.git"
  },
  "keywords": [
    "webpack",
    "react",
    "es6",
    "es2015",
    "babel",
    "airbnb",
    "eslint",
    "sass"
  ],
  "author": "Matt Richards",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "postcss-calc": "^5.3.1",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.11.1",
    "postcss-modules-values": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "react": "16.1.1",
    "react-dom": "16.1.1",
    "react-hot-loader": "3.1.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-combine-loaders": "^2.0.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react-router-dom": "4.2.2"
  }
}
